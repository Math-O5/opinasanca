<% all_sugestion.each do |sugestion| %>

  <div class="">
      <div class="small-12 medium-3 column">
        <div class="small-12 medium-6 column" data-equalizer-watch>
          <h4><%= link_to sugestion.title, sugestion %></h4>
          <div class="row">
            <%= sugestion.latitude %>
            <%= sugestion.longitude %>
          </div>
          <p><%= sugestion.display_name %></p>
        </div>
      </div>
    
  </div>  
<% end %>
